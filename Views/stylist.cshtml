<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
      <h3>@Model.GetName()</h3>
      <ul>
        <li>@Model.GetWorkHours()</li>
        <li>@Model.GetWorkSchedule()</li>
      </ul>
      @if(Model.GetClients().Count >0)
      {
        @foreach(var client in Model.GetClients())
        {
          <p>@client.GetClientName()</p>
        }
      }
    </ul>
      
      <a href="/clients/new">Add Client</a></br>
      <a href="/">Home Page</a>
  </body>
</html>
